{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "cross-env NODE_ENV=test",
    "start": "turbo web#start",
    "local-lint": "turbo local-lint"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^3.2.5",
    "turbo": "^2.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "force": "^0.0.3",
    "i18next": "^23.14.0",
    "next-i18next": "^15.3.1",
    "react-i18next": "^15.0.1"
  }
}
