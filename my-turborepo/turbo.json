{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["globalEnv"],
  "globalPassThroughEnv": ["Evan"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "web#lint": {
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "local-lint":{
    }
  }
}
